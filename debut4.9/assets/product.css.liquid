
/* Origianl CSS */
@font-face {
  font-family: 'Glyphicons Halflings';
  src: url('http://netdna.bootstrapcdn.com/bootstrap/3.0.0/fonts/glyphicons-halflings-regular.eot');
  src: url('http://netdna.bootstrapcdn.com/bootstrap/3.0.0/fonts/glyphicons-halflings-regular.eot?#iefix') format('embedded-opentype'), url('http://netdna.bootstrapcdn.com/bootstrap/3.0.0/fonts/glyphicons-halflings-regular.woff') format('woff'), url('http://netdna.bootstrapcdn.com/bootstrap/3.0.0/fonts/glyphicons-halflings-regular.ttf') format('truetype'), url('http://netdna.bootstrapcdn.com/bootstrap/3.0.0/fonts/glyphicons-halflings-regular.svg#glyphicons-halflingsregular') format('svg');
}
.glyphicon {
  position: relative;
  top: 1px;
  display: inline-block;
  font-family: 'Glyphicons Halflings';
  font-style: normal;
  font-weight: normal;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
}

/* Origianl CSS */
.hidden {
    display: none;
}
@media (max-width: 759px) {
    .mobileHidden {
        display: none;
   }
}
@media (min-width: 759px) {
  .desktopHidden {
      display: none;
 }
}

.product-specification-header {
    font-weight: 700;
}
.product-specification-list {
    list-style: disc outside;
}
.outside {
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
}
.product-description-inner {
    position: relative;
    z-index: 2;
}
.product-description-selection {
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid #d4d4d4;
}
.product-description-selection .active {
    font-weight: 800;
    border-bottom: 2px solid #2d2926;
}
.product-description-selection-option {
    width: 20%;
    text-align: center;
}
.product-description-selected {
    padding-bottom: 20px;
    padding-top: 20px;
}
/* Mobile CSS */
@media (max-width: 759px) {
  .product-description-selection-mobile {
    display: block;
    box-sizing: border-box;
  }
  .product-description-selection-option-mobile{
    display:flex;
    border-width: 1px 1px 0 1px;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    box-sizing: border-box;
    text-align: center;
    justify-content: space-between;
    overflow: hidden;
    position: relative;
    width: auto;
    height: 15px;
    padding: 12px 12px 12px 12px;
    margin: 0;
    background-color: #f1f1f1;
    border: 1px solid #dddddd;
    border-bottom: none;
    color: #8d8d8d;
    line-height: 15px;
    font-size: 15px;
    font-style: normal;
    font-weight: normal;
    border-radius: 0;
    outline: none;
    -webkit-box-sizing: content-box;
    -moz-box-sizing: content-box;
    box-sizing: content-box;
    cursor: pointer;
    text-overflow: ellipsis;
    white-space: nowrap;
    opacity: 1t
  }
  .product-description-selection-mobile .active{
    background-color: white;
  }
  .description-wrapper-mobile{
    border-width: 1px 1px 1px 1px;
    padding: 10px 12px 10px 12px;
    border-radius: 0 3px 3px 3px;
    border: 1px solid #dddddd;
    background-color: #ffffff;
    text-align: left;
    word-wrap: break-word;
    overflow-wrap: break-word
  }
  .description-wrapper-mobile .featured__product{
    background: white;
  }
}

.product-brand-wrapper {
    display: block;
    justify-content: center;
    align-items: center;
}
.brand-row {
    display: flex;
    justify-content: center;
    margin-bottom: 20px;
}
.product-slider {
    display: block;
    justify-content: center;
    align-items: center;
}
.product-slider h2 {
    text-align: center;
}
.product-slider a, .product-slider a:focus, .product-slider a:hover, .product-slider a:not([disabled]):hover {
    cursor: pointer;
    color: inherit;
}
.product-slider article {
    display: block;
    position: relative;
}
.product-slider a {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 2;
}
.product-slider .slider-btn {
    width: 4%;
    text-align: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -moz-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
    background: transparent;
    position: sticky;
    top: 0;
    bottom: 0;
    z-index: 20;
    align-items: start;
}
.product-slider .slider-left-button {
    left: 0;
}
.product-slider .slider-right-button {
    right: 0;
}
.slider-btn-wrapper {
    position: absolute;
}
.product-slider-window {
    display: flex;
    overflow-x: auto;
    position: relative;
}
.product-slider-window:hover .indicator-icon {
    position: absolute;
    display: block;
    transform-origin: 55% 50%;
    cursor: pointer;
    font-style: normal;
    height: auto;
    -webkit-align-self: center;
    -ms-flex-item-align: center;
    -ms-grid-row-align: center;
    align-self: center;
    font-size: 2.5vw;
    -webkit-transition: -webkit-transform 0.1s ease-out 0s;
    transition: -webkit-transform 0.1s ease-out 0s;
    -o-transition: -o-transform 0.1s ease-out 0s;
    -moz-transition: transform 0.1s ease-out 0s, -moz-transform 0.1s ease-out 0s;
    transition: transform 0.1s ease-out 0s;
    transition: transform 0.1s ease-out 0s, -webkit-transform 0.1s ease-out 0s, -moz-transform 0.1s ease-out 0s, -o-transform 0.1s ease-out 0s;
    font-family: 'Glyphicons Halflings';
    color: black;
}
.product-slider-window:hover .icon-leftCaret {
    left: 0;
}
.product-slider-window:hover .icon-rightCaret {
    right: 0;
}
.product-slider-window:hover .icon-leftCaret:before {
    content: "\e079";
}
.product-slider-window:hover .icon-rightCaret:after {
    content: "\e080";
}
.product-slider-window:after {
    display: block;
}
.product-slider-row{
    cursor:pointer;
    -webkit-box-flex: 0;
    -webkit-flex: 0 0 auto;
    -moz-box-flex: 0;
    -moz-flex: 0 0 auto;
    -ms-flex: 0 0 auto;
    flex: 0 0 auto;
    padding: 0 6px;
    box-sizing: border-box;
    padding-bottom: 10px;
    width: 25%;
    position: relative;
}
.innerlink{
    position:absolute;
    top:0;
    bottom:0;
    right:0; 
    left: 0;
    display:block;
}
.innerlink::after{
    content: "";
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
}
@media (max-width: 759px) {
    .product-slider-row {
        width: 45%;
   }
  .product-slider-window:hover .indicator-icon {
    color:transparent;
    font-size:30px;
  }
}
.product-slider-img-wrapper {
    padding-bottom: 100%;
    background-color: #f5f5f5;
    border-radius: 4px;
    position: relative;
}
.product-slider-img {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 268px;
    height: 268px;
    height: 100%;
    transform: translate(-50%, -50%);
    transition: transform 0.2s;
    border: 0;
}
@media (max-width: 759px) {
  .product-slider-img {
    width: 158px;
    height: 158px;
 }
}
.product-slider-info-wrapper {
    padding: 0 4px;
}
.product-slider-title {
    display: block;
    max-height: 34px;
    margin-top: 5px;
    font-size: 18px;
    word-break: break-all;
    font-weight: 400;
    line-height: 17px;
    color: #000;
    transition: opacity 0.1s;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    word-wrap: break-word;
    text-align: left;
    margin-bottom: 20px;
}
@media (max-width: 759px) {
    .product-slider-title {
        height: 34px;
        margin-bottom: 0px;
        font-size: 13px;
        -webkit-line-clamp: 2;
   }
}
@media (max-width: 759px) {
    .slider-header {
        margin: 20px;
   }
}
.product-slider-price-wrapper {
    display: flex;
    font-weight: 800;
}
@media (max-width: 759px) {
    .product-slider-price-wrapper {
        font-size: 0.93333em;
   }
}
.product-slider-price-wrapper .percent {
    color: #1878b9;
    margin-right: 5px;
}

